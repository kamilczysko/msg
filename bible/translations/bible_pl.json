rseNumber":"19","verseContent":""},{"verseNumber":"20","verseContent":""},{"verseNumber":"21","verseContent":""},{"verseNumber":"22","verseContent":""},{"verseNumber":"23","verseContent":""},{"verseNumber":"24","verseContent":""},{"verseNumber":"25","verseContent":""},{"verseNumber":"26","verseContent":""}]},{"chapterNumber":"18","verses":[{"verseNumber":"1","verseContent":""},{"verseNumber":"2","verseContent":""},{"verseNumber":"3","verseContent":""},{"verseNumber":"4","verseContent":""},{"verseNumber":"5","verseContent":""},{"verseNumber":"6","verseContent":""},{"verseNumber":"7","verseContent":""},{"verseNumber":"8","verseContent":""},{"verseNumber":"9","verseContent":""},{"verseNumber":"10","verseContent":""},{"verseNumber":"11","verseContent":""},{"verseNumber":"12","verseContent":""},{"verseNumber":"13","verseContent":""},{"verseNumber":"14","verseContent":""},{"verseNumber":"15","verseContent":""},{"verseNumber":"16","verseContent":""},{"verseNumber":"17","verseContent":""},{"verseNumber":"18","verseContent":""},{"verseNumber":"19","verseContent":""},{"verseNumber":"20","verseContent":""},{"verseNumber":"21","verseContent":""},{"verseNumber":"22","verseContent":""},{"verseNumber":"23","verseContent":""},{"verseNumber":"24","verseContent":""},{"verseNumber":"25","verseContent":""},{"verseNumber":"26","verseContent":""},{"verseNumber":"27","verseContent":""},{"verseNumber":"28","verseContent":""},{"verseNumber":"29","verseContent":""},{"verseNumber":"30","verseContent":""},{"verseNumber":"31","verseContent":""},{"verseNumber":"32","verseContent":""},{"verseNumber":"33","verseContent":""},{"verseNumber":"34","verseContent":""},{"verseNumber":"35","verseContent":""},{"verseNumber":"36","verseContent":""},{"verseNumber":"37","verseContent":""},{"verseNumber":"38","verseContent":""},{"verseNumber":"39","verseContent":""},{"verseNumber":"40","verseContent":""}]},{"chapterNumber":"19","verses":[{"verseNumber":"1","verseContent":""},{"verseNumber":"2","verseContent":""},{"verseNumber":"3","verseContent":""},{"verseNumber":"4","verseContent":""},{"verseNumber":"5","verseContent":""},{"verseNumber":"6","verseContent":""},{"verseNumber":"7","verseContent":""},{"verseNumber":"8","verseContent":""},{"verseNumber":"9","verseContent":""},{"verseNumber":"10","verseContent":""},{"verseNumber":"11","verseContent":""},{"verseNumber":"12","verseContent":""},{"verseNumber":"13","verseContent":""},{"verseNumber":"14","verseContent":""},{"verseNumber":"15","verseContent":""},{"verseNumber":"16","verseContent":""},{"verseNumber":"17","verseContent":""},{"verseNumber":"18","verseContent":""},{"verseNumber":"19","verseContent":""},{"verseNumber":"20","verseContent":""},{"verseNumber":"21","verseContent":""},{"verseNumber":"22","verseContent":""},{"verseNumber":"23","verseContent":""},{"verseNumber":"24","verseContent":""},{"verseNumber":"25","verseContent":""},{"verseNumber":"26","verseContent":""},{"verseNumber":"27","verseContent":""},{"verseNumber":"28","verseContent":""},{"verseNumber":"29","verseContent":""},{"verseNumber":"30","verseContent":""},{"verseNumber":"31","verseContent":""},{"verseNumber":"32","verseContent":""},{"verseNumber":"33","verseContent":""},{"verseNumber":"34","verseContent":""},{"verseNumber":"35","verseContent":""},{"verseNumber":"36","verseContent":""},{"verseNumber":"37","verseContent":""},{"verseNumber":"38","verseContent":""},{"verseNumber":"39","verseContent":""},{"verseNumber":"40","verseContent":""},{"verseNumber":"41","verseContent":""},{"verseNumber":"42","verseContent":""}]},{"chapterNumber":"20","verses":[{"verseNumber":"1","verseContent":""},{"verseNumber":"2","verseContent":""},{"verseNumber":"3","verseContent":""},{"verseNumber":"4","verseContent":""},{"verseNumber":"5","verseContent":""},{"verseNumber":"6","verseContent":""},{"verseNumber":"7","verseContent":""},{"verseNumber":"8","verseContent":""},{"verseNumber":"9","verseContent":""},{"verseNumber":"10","verseContent":""},{"verseNumber":"11","verseContent":""},{"verseNumber":"12","verseContent":""},{"verseNumber":"13","verseContent":""},{"verseNumber":"14","verseContent":""},{"verseNumber":"15","verseContent":""},{"verseNumber":"16","verseContent":""},{"verseNumber":"17","verseContent":""},{"verseNumber":"18","verseContent":""},{"verseNumber":"19","verseContent":""},{"verseNumber":"20","verseContent":""},{"verseNumber":"21","verseContent":""},{"verseNumber":"22","verseContent":""},{"verseNumber":"23","verseContent":""},{"verseNumber":"24","verseContent":""},{"verseNumber":"25","verseContent":""},{"verseNumber":"26","verseContent":""},{"verseNumber":"27","verseContent":""},{"verseNumber":"28","verseContent":""},{"verseNumber":"29","verseContent":""},{"verseNumber":"30","verseContent":""},{"verseNumber":"31","verseContent":""}]},{"chapterNumber":"21","verses":[{"verseNumber":"1","verseContent":""},{"verseNumber":"2","verseContent":""},{"verseNumber":"3","verseContent":""},{"verseNumber":"4","verseContent":""},{"verseNumber":"5","verseContent":""},{"verseNumber":"6","verseContent":""},{"verseNumber":"7","verseContent":""},{"verseNumber":"8","verseContent":""},{"verseNumber":"9","verseContent":""},{"verseNumber":"10","verseContent":""},{"verseNumber":"11","verseContent":""},{"verseNumber":"12","verseContent":""},{"verseNumber":"13","verseContent":""},{"verseNumber":"14","verseContent":""},{"verseNumber":"15","verseContent":""},{"verseNumber":"16","verseContent":""},{"verseNumber":"17","verseContent":""},{"verseNumber":"18","verseContent":""},{"verseNumber":"19","verseContent":""},{"verseNumber":"20","verseContent":""},{"verseNumber":"21","verseContent":""},{"verseNumber":"22","verseContent":""},{"verseNumber":"23","verseContent":""},{"verseNumber":"24","verseContent":""},{"verseNumber":"25","verseContent":""}]}]},{"bookName":"acts","chapters":[{"chapterNumber":"1","verses":[{"verseNumber":"1","verseContent":"Drogi Teofil, w pierwszym tomie tej ksi\u0105\u017cki b\u0119d\u0119 pisa\u0142 o wszystkim, co Jezus zacz\u0105\u0142 czyni\u0107 i naucza\u0107"},{"verseNumber":"2","verseContent":"podczas gdy on \u017cegna\u0142 si\u0119 z aposto\u0142ami, kt\u00f3rych on wybra\u0142 Duchem \u015awi\u0119tym, a zosta\u0142 wzi\u0119ty do nieba."},{"verseNumber":"3","verseContent":"Po jego \u015bmierci, przedstawi\u0142 si\u0119 przy \u017cyciu dla nich w wielu przypadkach w ci\u0105gu czterdziestu dni. W imprezach spotkaniach, m\u00f3wi\u0142 z nimi o rzeczach, o Kr\u00f3lestwie Bo\u017cym."},{"verseNumber":"4","verseContent":"Jak zbierali si\u0119 razem i jedli posi\u0142ek, powiedzia\u0142 im, \u017ce oni w \u017cadnym wypadku nie opuszczali Jerozolimy, ale \"trzeba poczeka\u0107, \u017ce ojciec obieca\u0142: s\u0142owo, kt\u00f3re s\u0142yszeli\u015bcie ode mnie."},{"verseNumber":"5","verseContent":"Jan chrzci\u0142 w wodzie"},{"verseNumber":"6","verseContent":"Gdy byli razem, ostatni raz pytali: \"Mistrzu, masz zamiar odbudowa\u0107 kr\u00f3lestwo Izraela teraz? To jest czas?\""},{"verseNumber":"7","verseContent":"Powiedzia\u0142 im, \"nie dostaniesz, aby dowiedzie\u0107 si\u0119 czas. Czas na biznes ojca."},{"verseNumber":"8","verseContent":"Co dostaniesz, to Duch \u015awi\u0119ty. I gdy Duch \u015awi\u0119ty zst\u0105pi na was, mo\u017cecie by\u0107 moimi \u015bwiadkami w Jerozolimie, w ca\u0142ej Judei, i w Samarii, i a\u017c po kra\u0144ce ziemi\"."},{"verseNumber":"9","verseContent":"To by\u0142y jego ostatnie s\u0142owa. Jak wygl\u0105da\u0142y, wsta\u0142 i znikn\u0105\u0142 w chmurze."},{"verseNumber":"10","verseContent":"Stali, wpatruj\u0105c si\u0119 w puste niebo. Nagle pojawi\u0142y si\u0119 dw\u00f3ch m\u0119\u017cczyzn w bia\u0142ych szatach!"},{"verseNumber":"11","verseContent":"Oni powiedzieli: \"Galilejczycy! - dlaczego po prostu sta\u0107 tutaj, patrz\u0105c na puste niebo? Ten sam Jezus, kt\u00f3ry w g\u00f3r\u0119 wzi\u0119ty jest od was do nieba, przyjdzie na pewno - i tajemniczo - jak odszed\u0142\"."},{"verseNumber":"12","verseContent":"Tak opu\u015bcili g\u00f3ry, zwanej Oliwnej, i wr\u00f3ci\u0142 do Jerozolimy. To by\u0142o troch\u0119 bardziej \u043f\u043e\u043b\u0432\u0435\u0440\u0441\u0442\u044b."},{"verseNumber":"13","verseContent":"Wzros\u0142y one w g\u00f3rnej pokoju u\u017cywali jako Miejsce spotkania: Piotr i Jakub, i Jan, i Andrzej, Filip i Tomasz, Bart\u0142omiej i Mateusz, Jakub, syn Alfeusza, Szymon Zelota, Juda, syn Jakuba."},{"verseNumber":"14","verseContent":"Zgodzili si\u0119, byli w tym dobrym, w pe\u0142ni razem w modlitwie, w tym kobiet. Tak\u017ce Matka Jezusa, Maria, i jego bracia. Wymiana Judasz"},{"verseNumber":"15","verseContent":"W tym czasie Piotr wsta\u0142 w firmie - tam by\u0142o oko\u0142o stu dwudziestu z nich w pokoju w tym czasie"},{"verseNumber":"16","verseContent":"i powiedzia\u0142: \"Przyjaciele, dawno, dawno temu Duch \u015awi\u0119ty m\u00f3wi\u0142 przez Dawida o Judaszu, kt\u00f3ry sta\u0142 si\u0119 przewodnikiem dla tych, kt\u00f3rzy aresztowa\u0142 Jezusa. To pismo mia\u0142o si\u0119 spe\u0142ni\u0107, i teraz by\u0142a."},{"verseNumber":"17","verseContent":"Judasz by\u0142 jednym z nas i jego wydzielone miejsce w tej s\u0142u\u017cbie."},{"verseNumber":"18","verseContent":"\"Jak wiesz, \u017ce to jest z\u0142o wzi\u0119\u0142a \u0142ap\u00f3wk\u0119 pieni\u0119dzy i kupi\u0107 ma\u0142\u0105 farm\u0119. Tam jest \u017ale sko\u0144czy\u0142, szarpi\u0105c go brzuch i wypadaj\u0105 grubego."},{"verseNumber":"19","verseContent":"Ka\u017cdy w Jerozolimie to wie"},{"verseNumber":"20","verseContent":"To jest dok\u0142adnie to, co widzimy, jest napisane w ksi\u0119dze Psalm\u00f3w: niech jego gospodarstwa staj\u0105 si\u0119 duchami, tak, \u017ce nikt nigdy nie b\u0119dzie w stanie tam mieszka\u0107. \"A tak\u017ce to, co zosta\u0142o napisane p\u00f3\u017aniej: niech kto\u015b inny zajmie jego stanowisko."},{"verseNumber":"21","verseContent":"\"Judasz teraz musi by\u0107 wymieniony. Wymiana musi pochodzi\u0107 od firmy ludzi, kt\u00f3rzy razem z nami"},{"verseNumber":"22","verseContent":"od tego czasu Jezus przyj\u0105\u0142 chrzest od Jana w dniu swego wniebowst\u0105pienia, miejsca razem z nami \u015bwiadkiem Jego Zmartwychwstania\"."},{"verseNumber":"23","verseContent":"Oni posuwali si\u0119 naprz\u00f3d dw\u00f3ch: J\u00f3zefa \u0412\u0430\u0440\u0441\u0430\u0432\u043e\u0439, znany \u0418\u0443\u0441\u0442\u043e\u043c, i Macieja."},{"verseNumber":"24","verseContent":"Wtedy \u0432\u0437\u043c\u043e\u043b\u0438\u043b\u0438\u0441\u044c: \"Ty, Bo\u017ce, wiem, \u017ce ka\u017cdy z nas w \u015brodku i na zewn\u0105trz. Jasne, kt\u00f3ry z tych dw\u00f3ch m\u0119\u017cczyzn wybierzesz"},{"verseNumber":"25","verseContent":"aby zaj\u0105\u0107 miejsce w tej s\u0142u\u017cbie i podr\u0119czniki, kt\u00f3re Judasz rzuci\u0142 w tym, aby i\u015b\u0107 w\u0142asn\u0105 drog\u0105\"."},{"verseNumber":"26","verseContent":"Potem ci\u0105gn\u0119li s\u0142omki. Matthias wygra\u0142 i by\u0142 liczony w jedenastu aposto\u0142\u00f3w."}]},{"chapterNumber":"2","verses":[{"verseNumber":"1","verseContent":"sdfsdf"},{"verseNumber":"2","verseContent":"Bez \u017cadnego ostrze\u017cenia rozleg\u0142 si\u0119 d\u017awi\u0119k, jakby silny wiatr, huraganu si\u0142y - nikt nie m\u00f3g\u0142 powiedzie\u0107, sk\u0105d si\u0119 wzi\u0105\u0142. On nape\u0142ni\u0142 ca\u0142y dom."},{"verseNumber":"3","verseContent":"Nast\u0119pnie, jak po\u017car, Duch \u015awi\u0119ty rozprzestrzenia si\u0119 przez ich szeregi,"},{"verseNumber":"4","verseContent":"i zacz\u0119li m\u00f3wi\u0107 w r\u00f3\u017cnych j\u0119zykach, jak Duch natchn\u0105\u0142 ich."},{"verseNumber":"5","verseContent":"By\u0142o du\u017co \u017cyd\u00f3w, mieszkaj\u0105cych w Jerozolimie tylko wtedy, pobo\u017cni pielgrzymi z ca\u0142ego \u015bwiata."},{"verseNumber":"6","verseContent":"Gdy us\u0142yszeli d\u017awi\u0119k, przybyli na met\u0119. Nast\u0119pnie, gdy us\u0142yszeli, jedna po drugiej, ojczystym j\u0119zyku m\u00f3wi\u0105, byli oszo\u0142omieni."},{"verseNumber":"7","verseContent":"Nie mogli dla ich \u017cycia, dowiedzie\u0107 si\u0119, co si\u0119 dzieje, i ci\u0105gle m\u00f3wi: \"czy nie wszystko Galilejczycy?"},{"verseNumber":"8","verseContent":"Jak s\u0142yszymy ich rozmowy, w naszych rodzimych j\u0119zykach?"},{"verseNumber":"9","verseContent":"Partowie, med\u00f3w, i \u044d\u043b\u0430\u043c\u0438\u0442\u044b"},{"verseNumber":"10","verseContent":"Frygii i \u041f\u0430\u043c\u0444\u0438\u043b\u0438\u0438, Egiptu i cz\u0119\u015bci Libii, nale\u017c\u0105cych do \u041a\u0438\u0440\u0438\u043d\u0435\u0435"},{"verseNumber":"11","verseContent":"zar\u00f3wno \u017cydzi i prozelici"},{"verseNumber":"12","verseContent":"Ich g\u0142owy kr\u0119ci\u0142y"},{"verseNumber":"13","verseContent":"Inni \u017cartowali, \"byli pijani taniego wina\"."},{"verseNumber":"14","verseContent":"Wtedy Piotr wsta\u0142 i opieraj\u0105c si\u0119 na pozosta\u0142e jedena\u015bcie, wyst\u0105pi\u0142 ze \u015bmia\u0142ym znaczenie: \"towarzysze \u017cydzi, wszyscy, kt\u00f3rzy przybywaj\u0105 do Jerozolimy, uwa\u017cnie s\u0142ucha\u0107 i robi\u0107 to wszystko."},{"verseNumber":"15","verseContent":"Ci ludzie nie s\u0105 pijani, jak podejrzewasz. Oni jeszcze nie zd\u0105\u017cyli si\u0119 napi\u0107 - to jest dopiero dziewi\u0105ta rano."},{"verseNumber":"16","verseContent":"To jest to, co prorok Joel og\u0142osi\u0142 si\u0119 stanie:"},{"verseNumber":"17","verseContent":"\"W ostatnich dniach\" B\u00f3g m\u00f3wi: \"wylej\u0119 Mojego Ducha na wszelkiego rodzaju ludzie: synowie twoi b\u0119d\u0105 prorokowa\u0107, i twoich c\u00f3rek"},{"verseNumber":"18","verseContent":"Kiedy nadejdzie czas, wylej\u0119 Mojego Ducha na tych, kt\u00f3rzy s\u0142u\u017c\u0105 mi, i m\u0119\u017cczy\u017ani, i kobiety, i b\u0119d\u0105 prorokowa\u0107."},{"verseNumber":"19","verseContent":"Jestem cuda na niebie w g\u00f3rze i znaki w dole na ziemi, krew i ogie\u0144 i dym,"},{"verseNumber":"20","verseContent":"s\u0142o\u0144ce staje si\u0119 czarny, i Ksi\u0119\u017cyc krwistoczerwone, zanim nadejdzie dzie\u0144 Pa\u0144ski nadchodzi, dzie\u0144 wielki i wspania\u0142y"},{"verseNumber":"21","verseContent":"I ten, kto zwraca si\u0119 o pomoc do mnie, Boga, b\u0119dzie zbawiony\"."},{"verseNumber":"22","verseContent":"\"Bracia-izraelowi, uwa\u017cnie pos\u0142uchajcie tych s\u0142\u00f3w: Jezusa Nazarejczyka, cz\u0142owieka, starannie, akredytowane B\u00f3g dla was mocami i cudami i znakami, kt\u00f3re B\u00f3g stworzy\u0142 przez niego wiedza og\u00f3lna -"},{"verseNumber":"23","verseContent":"ten Jezus planu, w wyniku \u015bwiadomego i przemy\u015blanego Boga, zdradzili ludzi, kt\u00f3rzy wzi\u0119li sprawiedliwo\u015b\u0107 w swoje r\u0119ce, i zosta\u0142a przekazana pa\u0144stwu. I przybi\u0142 go do krzy\u017ca i zabili\u015bcie go."},{"verseNumber":"24","verseContent":"Ale B\u00f3g \u043e\u0442\u0432\u044f\u0437\u044b\u0432\u0430\u0435\u043c liny \u015bmierci i wskrzesi\u0142 go. \u015amier\u0107 by\u0142a mu nie dor\u00f3wnuje."},{"verseNumber":"25","verseContent":"David powiedzia\u0142, \u017ce to wszystko widzia\u0142em Boga przede mn\u0105 przez ca\u0142y czas. Nic nie mo\u017ce zachwia\u0107 mnie"},{"verseNumber":"26","verseContent":"Ciesz\u0119 si\u0119, \u017ce wewn\u0105trz, w orginale"},{"verseNumber":"27","verseContent":"Wiem, \u017ce nigdy mnie nie zostawisz w piek\u0142o"},{"verseNumber":"28","verseContent":"Masz moje nogi na drodze \u017cycia, z twoja twarz \u015bwieci s\u0142o\u0144ce-rado\u015b\u0107 wszystkim wok\u00f3\u0142."},{"verseNumber":"29","verseContent":"\"Drodzy przyjaciele, Pozw\u00f3lcie mi by\u0107 ca\u0142kowicie szczery z wami. Nasz przodek Dawid umar\u0142 i zosta\u0142 pochowany, jego gr\u00f3b znajduje si\u0119 do dzi\u015b w oczywisty spos\u00f3b."},{"verseNumber":"30","verseContent":"Ale by\u0107 tak\u017ce prorokiem i wiedz\u0105c, \u017ce B\u00f3g z przysi\u0119g\u0105 przysi\u0105g\u0142, \u017ce potomstwo jego b\u0119dzie rz\u0105dzi\u0107 w swoim kr\u00f3lestwie,"},{"verseNumber":"31","verseContent":"widz\u0105c daleko do przodu, m\u00f3wi\u0142 o zmartwychwstaniu Mesjasza - '\u017cadna podr\u00f3\u017c do piek\u0142a, ani smr\u00f3d \u015bmierci.'"},{"verseNumber":"32","verseContent":"Tego Jezusa wskrzesi\u0142 B\u00f3g. I ka\u017cdy z nas tutaj jest tego \u015bwiadkiem."},{"verseNumber":"33","verseContent":"Potem, podnie\u015b\u0107 do wysoko\u015bci po prawicy Boga i otrzyma\u0142 obietnic\u0119 Ducha \u015awi\u0119tego od Ojca, wyla\u0142 Ducha dosta\u0142 tylko. To jest to, co widzisz i s\u0142yszysz."},{"verseNumber":"34","verseContent":"Bo sam Dawid nie wst\u0105pi\u0142 do nieba, ale on powiedzia\u0142, B\u00f3g powiedzia\u0142 do mojego pana: \"sied\u017a na praw\u0105 r\u0119k\u0119"},{"verseNumber":"35","verseContent":"Dop\u00f3ki nie zrobi\u0119 twoich wrog\u00f3w sto\u0142ek odpoczynku dla n\u00f3g.\""},{"verseNumber":"36","verseContent":"\"Ca\u0142y Izrael, to wiedz: nie ma ju\u017c miejsca na w\u0105tpliwo\u015bci - B\u00f3g uczyni\u0142 go mistrzem i mesjaszem tego Jezusa, kt\u00f3rego wy\u015bcie zabili na krzy\u017cu.\""},{"verseNumber":"37","verseContent":"No\u017cyczkami, ci, kt\u00f3rzy tam byli, s\u0142uchaj\u0105c, zapyta\u0142 Piotra i innych aposto\u0142\u00f3w: \"bracia! Bracia! Wi\u0119c co teraz zrobimy?\""},{"verseNumber":"38","verseContent":"Piotr powiedzia\u0142: \"zmie\u0144 swoje \u017cycie. Zwr\u00f3\u0107 si\u0119 do Boga, i niechaj si\u0119 ka\u017cdy z was w imi\u0119 Jezusa Chrystusa, aby twoje grzechy s\u0105 odpuszczone. Otrzymacie dar Ducha \u015awi\u0119tego."},{"verseNumber":"39","verseContent":"Obietnica dla ciebie i twoich dzieci, ale i dla wszystkich, kt\u00f3rzy s\u0105 daleko - tego, kto w rzeczywisto\u015bci, nasz pan B\u00f3g zaprasza\"."},{"verseNumber":"40","verseContent":"Poszed\u0142 dalej w tym kierunku przez d\u0142u\u017cszy czas, wzywaj\u0105c je ponownie i ponownie: \"uciekaj, p\u00f3ki mo\u017cesz"},{"verseNumber":"41","verseContent":"W tym dniu oko\u0142o trzech tysi\u0119cy wzi\u0119\u0142a go w jego s\u0142owa, zostali ochrzczeni i zosta\u0142y podpisane."},{"verseNumber":"42","verseContent":"Oni zobowi\u0105zali si\u0119 w nauce aposto\u0142\u00f3w, we wsp\u00f3lnym \u017cyciu, wsp\u00f3lnym posi\u0142ku i modlitwy."},{"verseNumber":"43","verseContent":"Wszyscy wok\u00f3\u0142 byli zachwyceni - wszystkie te cuda i znaki przez aposto\u0142\u00f3w!"},{"verseNumber":"44","verseContent":"I wszyscy wierz\u0105cy \u017cyli w pi\u0119knej harmonii, trzymaj\u0105c wszystko wsp\u00f3lne."},{"verseNumber":"45","verseContent":"Sprzedawali wszystko, co do nich nale\u017cy i po\u0142\u0105czyli swoje zasoby tak, \u017ce dla ka\u017cdego cz\u0142owieka by\u0142a potrzebna."},{"verseNumber":"46","verseContent":"Sz\u0142y codziennie dyscypliny nabo\u017ce\u0144stwa w \u015bwi\u0105tyni nast\u0105pi\u0142y obiady w domu, ka\u017cdy posi\u0142ek w wakacje, \u017cywio\u0142owy i radosny,"},{"verseNumber":"47","verseContent":"jak oni wielbili Boga. Ludzie w og\u00f3le podoba\u0142o si\u0119 to, co zobaczyli. Ka\u017cdego dnia ich liczba wci\u0105\u017c ro\u015bnie, jak B\u00f3g doda\u0142 Ci, kt\u00f3rzy zostali zbawieni."}]},{"chapterNumber":"3","verses":[{"verseNumber":"1","verseContent":"Pewnego dnia w trzy godziny dnia Piotr i Jan udali si\u0119 do \u015bwi\u0105tyni na spotkanie modlitewne."},{"verseNumber":"2","verseContent":"W tym samym czasie by\u0142 cz\u0142owiek kulawy od urodzenia jest do. Ka\u017cdy dzie\u0144 siedzia\u0142 w bramie \u015bwi\u0105tyni, w imieniu pi\u0119kne, aby ja\u0142mu\u017cn\u0119 przychodz\u0105cych do \u015bwi\u0105tyni."},{"verseNumber":"3","verseContent":"Kiedy zobaczy\u0142 Piotra i Jana, aby wej\u015b\u0107 do \u015bwi\u0105tyni, prosi\u0142 o ja\u0142mu\u017cn\u0119."},{"verseNumber":"4","verseContent":"Piotr wraz z Janem w jego stron\u0119, spojrza\u0142 mu prosto w oczy i powiedzia\u0142: \"zobacz To.\""},{"verseNumber":"5","verseContent":"sdfsdfs"},{"verseNumber":"6","verseContent":"Piotr powiedzia\u0142: \"nie Jestem grosz, aby moje imi\u0119, ale co mam to ci daj\u0119: w imi\u0119 Jezusa Chrystusa nazarejczyka, chod\u017a!\""},{"verseNumber":"7","verseContent":"Chwyci\u0142 go za praw\u0105 r\u0119k\u0119 i wyci\u0105gn\u0105\u0142 go. W jednej chwili jego nogi sta\u0142y si\u0119 mocniejsze."},{"verseNumber":"8","verseContent":"Zerwa\u0142 si\u0119 na nogi i poszed\u0142."},{"verseNumber":"9","verseContent":"Wszyscy widzieli, jak on chodzi i chwali w niej Boga."},{"verseNumber":"10","verseContent":"Uznali go, jak i ten, kt\u00f3ry siedzia\u0142 i prosi\u0142 o ja\u0142mu\u017cn\u0119 u \u015bwi\u0105tyni bram pi\u0119kne i przetar\u0142 oczy, wstrz\u0105\u015bni\u0119ty, ledwie wierz\u0105c temu, co widzieli."},{"verseNumber":"11","verseContent":"M\u0119\u017cczyzna obj\u0105\u0142 Piotra i Jana, ekstatyczny. Wszyscy ludzie s\u0105 tam, gdzie by\u0142y one w przedsionku Salomona, aby zobaczy\u0107 go dla siebie."},{"verseNumber":"12","verseContent":"Kiedy Piotr zobaczy\u0142 jego spotkanie, zwr\u00f3ci\u0142 si\u0119 do ludu:"},{"verseNumber":"13","verseContent":"B\u00f3g Abrahama i Izaaka i Jakuba, B\u00f3g ojc\u00f3w naszych, uwielbi\u0142 Syna Swego, Jezusa. Ten, kt\u00f3ry powo\u0142a\u0142 Pi\u0142at niewinny, to \u043e\u0442\u043a\u0440\u0435\u0449\u0438\u0432\u0430\u043b\u0438\u0441\u044c."},{"verseNumber":"14","verseContent":"Mo\u017cna wyrzekli \u015bwi\u0119tego, sprawiedliwego i poprosili morderca jest na swoim miejscu."},{"verseNumber":"15","verseContent":"Nie pr\u0119dzej zabili tw\u00f3rcy \u017cycia, ni\u017c B\u00f3g wzbudzi\u0142 go z martwych, i jeste\u015bmy \u015bwiadkami."},{"verseNumber":"16","verseContent":"Wiara w imi\u0119 Jezusa wsadzi\u0107 tego cz\u0142owieka, kt\u00f3rego stan mo\u017cna tak dobrze znasz, na nogach - tak, wiara i nic opr\u00f3cz wiary umie\u015bci\u0107 ten cz\u0142owiek zosta\u0142 uzdrowiony i wszystko przed oczami."},{"verseNumber":"17","verseContent":"\"A teraz, przyjaciele, wiem, \u017ce nie mia\u0142 poj\u0119cia, co robisz, kiedy zabili Jezusa, a nie wasi przyw\u00f3dcy."},{"verseNumber":"18","verseContent":"Ale B\u00f3g, kt\u00f3ry poprzez g\u0142oszenie wszystkich prorok\u00f3w powiedzia\u0142, \u017ce jego Mesjasz b\u0119dzie zabity, dok\u0142adnie wiedzieli, co robili i u\u017cywali go, aby zrealizowa\u0107 swoje plany."},{"verseNumber":"19","verseContent":"\"Teraz nadszed\u0142 czas, aby zmieni\u0107 swoje sposoby! Obr\u00f3ci\u0107 twarz\u0105 do Boga, aby on m\u00f3g\u0142 wymaza\u0107 swoje grzechy, wyla\u0107 deszcz b\u0142ogos\u0142awie\u0144stwa, aby od\u015bwie\u017cy\u0107 ciebie,"},{"verseNumber":"20","verseContent":"i wy\u015bl\u0119 ci Mesjasza, on przygotowa\u0142 dla ciebie, a mianowicie Jezus."},{"verseNumber":"21","verseContent":"Na razie musi pozosta\u0107 poza zasi\u0119giem wzroku w niebo, a\u017c wszystko zostanie przywr\u00f3cone do tego, znowu po prostu tak, jak B\u00f3g poprzez g\u0142oszenie \u015bwi\u0119tych prorok\u00f3w swoich starych, powiedzia\u0142, \u017ce b\u0119dzie."},{"verseNumber":"22","verseContent":"Moj\u017cesz, na przyk\u0142ad, powiedzia\u0142, \" B\u00f3g tw\u00f3j wzbudzi ci proroka, jak mnie od swojej rodziny. S\u0142ucha\u0107 ka\u017cdego s\u0142owa, kt\u00f3re m\u00f3wi do ciebie."},{"verseNumber":"23","verseContent":"Ka\u017cda \u017cywa dusza, kt\u00f3rzy nie chc\u0105 s\u0142ucha\u0107 tego proroka, zostanie usuni\u0119te z ludu'."},{"verseNumber":"24","verseContent":"\"Wszyscy prorocy od Samuela i dalej powiedzia\u0142 to samo, powiedzia\u0142 zdecydowanie, \u017ce te dni nadejd\u0105."},{"verseNumber":"25","verseContent":"Te prorocy, wraz z przymierzem, kt\u00f3re B\u00f3g zawar\u0142 z waszymi przodkami, to twoje drzewo genealogiczne. Przymierze boga-s\u0142owo Abrahamowi us\u0142ug tekst: w nasieniu twoim wszystkie narody ziemi b\u0119d\u0105 b\u0142ogos\u0142awione.'"},{"verseNumber":"26","verseContent":"Ale nie jeste\u015b pierwszy w kolejce: B\u00f3g, gdy wskrzesi\u0142 syna swego, pos\u0142a\u0142 go, aby wam b\u0142ogos\u0142awi\u0142, jak po kolei, jeden po drugim, od swoich z\u0142ych dr\u00f3g.\""}]},{"chapterNumber":"4","verses":[{"verseNumber":"1","verseContent":""},{"verseNumber":"2","verseContent":""},{"verseNumber":"3","verseContent":""},{"verseNumber":"4","verseContent":""},{"verseNumber":"5","verseContent":""},{"verseNumber":"6","verseContent":""},{"verseNumber":"7","verseContent":""},{"verseNumber":"8","verseContent":""},{"verseNumber":"9","verseContent":""},{"verseNumber":"10","verseContent":""},{"verseNumber":"11","verseContent":""},{"verseNumber":"12","verseContent":""},{"verseNumber":"13","verseContent":""},{"verseNumber":"14","verseContent":""},{"verseNumber":"15","verseContent":""},{"verseNumber":"16","verseContent":""},{"verseNumber":"17","verseContent":""},{"verseNumber":"18","verseContent":""},{"verseNumber":"19","verseContent":""},{"verseNumber":"20","verseContent":""},{"verseNumber":"21","verseContent":""},{"verseNumber":"22","verseContent":""},{"verseNumber":"23","verseContent":""},{"verseNumber":"24","verseContent":""},{"verseNumber":"25","verseContent":""},{"verseNumber":"26","verseContent":""},{"verseNumber":"27","verseContent":""},{"verseNumber":"28","verseContent":""},{"verseNumber":"29","verseContent":""},{"verseNumber":"30","verseContent":""},{"verseNumber":"31","verseContent":""},{"verseNumber":"32","verseContent":""},{"verseNumber":"33","verseContent":""},{"verseNumber":"34","verseContent":""},{"verseNumber":"35","verseContent":""},{"verseNumber":"36","verseContent":""},{"verseNumber":"37","verseContent":""}]},{"chapterNumber":"5","verses":[{"verseNumber":"1","verseContent":""},{"verseNumber":"2","verseContent":""},{"verseNumber":"3","verseContent":""},{"verseNumber":"4","verseContent":""},{"verseNumber":"5","verseContent":""},{"verseNumber":"6","verseContent":""},{"verseNumber":"7","verseContent":""},{"verseNumber":"8","verseContent":""},{"verseNumber":"9","verseContent":""},{"verseNumber":"10","verseContent":""},{"verseNumber":"11","verseContent":""},{"verseNumber":"